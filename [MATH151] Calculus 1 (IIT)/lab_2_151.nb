 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"f", "(", 
     RowBox[{"g", "(", "x", ")"}], ")"}], "=", 
    RowBox[{"-", "x"}]}], TraditionalForm]]],
