  RowBox[{
   RowBox[{"-", 
    RowBox[{"Cot", "[", "t", "]"}]}], ",", "t"}], "]"}]], "Input",
 CellChangeTimes->{{3.63317948605539*^9, 3.633179494153782*^9}}],

