Cell[TextData[{
 "1.\tBy playing with the sliders above, find the value of the approximate \
value of ",
 Cell[BoxData[
  FormBox["x", TraditionalForm]]],
 " for which the total distance traveled is a minimum for our situation in \
which \t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"a", "=", "4"}], ",", " ", 
    RowBox[{"b", "=", "2"}], ",", " ", 
    RowBox[{
     RowBox[{"and", " ", "c"}], "=", "6"}]}], TraditionalForm]]],
 ". \n\nx = ",
 "3.95"
}], "Text",
 CellChangeTimes->{{3.491837001624*^9, 3.4918370020959997`*^9}, {
   3.49183748663*^9, 3.4918375471219997`*^9}, {3.5695948653640003`*^9, 
