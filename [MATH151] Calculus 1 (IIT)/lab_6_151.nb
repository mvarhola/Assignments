         RowBox[{"Grid", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{
             RowBox[{"{", 
              RowBox[{
              "\"\<exact\>\"", ",", "\"\<approx\>\"", ",", "\"\<error\>\"", 
               ",", " ", "\"\<opt s\>\""}], "}"}], ",", 
             RowBox[{"{", 
              RowBox[{
               RowBox[{"ToString", "[", 
                RowBox[{"exact", "//", "Chop"}], "]"}], ",", 
