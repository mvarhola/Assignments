Cell[CellGroupData[{

Cell["A Reimann Sum Manipulate", "Subsection",
 CellChangeTimes->{{3.6361996898236966`*^9, 3.6361996942391376`*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Manipulate", "[", 
  RowBox[{
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "x", ",", "xstar", ",", "\[CapitalDelta]x", ",", 
       "\[DoubleStruckCapitalP]", ",", "s", ",", "tt", ",", "exact", ",", 
       "approx", ",", "sval", ",", "p1", ",", "p2"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{"x", "[", "i_", "]"}], ":=", 
       RowBox[{
        RowBox[{"a", "+", 
         RowBox[{
          FractionBox[
           RowBox[{"(", 
            RowBox[{"b", "-", "a"}], ")"}], "n"], "i"}]}], "//", "N"}]}], ";",
       "\[IndentingNewLine]", 
      RowBox[{"\[DoubleStruckCapitalP]", "=", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"x", "[", "i", "]"}], ",", 
         RowBox[{"{", 
          RowBox[{"i", ",", "0", ",", "n"}], "}"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"xstar", "[", 
        RowBox[{"i_", ",", "s_"}], "]"}], ":=", 
       RowBox[{
        RowBox[{
         RowBox[{"(", 
          RowBox[{"1", "-", "s"}], ")"}], 
         RowBox[{"x", "[", 
          RowBox[{"i", "-", "1"}], "]"}]}], "+", 
        RowBox[{"s", " ", 
         RowBox[{"x", "[", "i", "]"}]}]}]}], ";", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"\[CapitalDelta]x", "[", "i_", "]"}], ":=", 
       RowBox[{
        RowBox[{"x", "[", 
         RowBox[{"i", "+", "1"}], "]"}], "-", 
        RowBox[{"x", "[", "i", "]"}]}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"exact", "=", 
       RowBox[{"NIntegrate", "[", 
        RowBox[{
         RowBox[{"f", "[", "x", "]"}], ",", 
         RowBox[{"{", 
          RowBox[{"x", ",", "a", ",", "b"}], "}"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"tt", "=", 
       RowBox[{"Switch", "[", 
        RowBox[{
        "type", ",", "\"\<left\>\"", ",", "0", ",", "\"\<mid\>\"", ",", 
         FractionBox["1", "2"], ",", "\"\<right\>\"", ",", "1", ",", 
         "\"\<free\>\"", ",", "t"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"approx", "=", 
       RowBox[{
        UnderoverscriptBox["\[Sum]", 
         RowBox[{"i", "=", "1"}], "n"], 
        RowBox[{
         RowBox[{"f", "[", 
          RowBox[{"xstar", "[", 
           RowBox[{"i", ",", "tt"}], "]"}], "]"}], 
         RowBox[{"\[CapitalDelta]x", "[", "i", "]"}]}]}]}], ";", 
      RowBox[{"sval", "=", 
       RowBox[{"s", "/.", 
        RowBox[{"Quiet", "@", 
         RowBox[{
          RowBox[{"NSolve", "[", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{
              RowBox[{
               RowBox[{
                UnderoverscriptBox["\[Sum]", 
                 RowBox[{"i", "=", "1"}], "n"], 
                RowBox[{
                 RowBox[{"f", "[", 
                  RowBox[{"xstar", "[", 
                   RowBox[{"i", ",", "s"}], "]"}], "]"}], 
                 RowBox[{"\[CapitalDelta]x", "[", "i", "]"}]}]}], "\[Equal]", 
               "exact"}], ",", 
              RowBox[{
              "0", "\[LessEqual]", " ", "s", "\[LessEqual]", " ", "1"}]}], 
             "}"}], ",", "s"}], "]"}], "[", 
          RowBox[{"[", "1", "]"}], "]"}]}]}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"Plot", "[", 
       RowBox[{
        RowBox[{"f", "[", "x", "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"x", ",", "a", ",", "b"}], "}"}], ",", 
        RowBox[{"PlotRange", "\[Rule]", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"0", ",", "4"}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{"0", ",", "10"}], "}"}]}], "}"}]}], ",", 
        RowBox[{"PlotStyle", "\[Rule]", "Thick"}], ",", 
        RowBox[{"AxesOrigin", "\[Rule]", 
         RowBox[{"{", 
          RowBox[{"0", ",", "0"}], "}"}]}], ",", " ", 
        RowBox[{"Axes", "\[Rule]", " ", "True"}], ",", 
        RowBox[{"Prolog", "\[Rule]", 
         RowBox[{"Prepend", "[", 
          RowBox[{
           RowBox[{"Prepend", "[", 
            RowBox[{
             RowBox[{
              RowBox[{
               RowBox[{"Rectangle", "[", 
                RowBox[{
                 RowBox[{"{", 
                  RowBox[{
                   RowBox[{"#", "[", 
                    RowBox[{"[", "1", "]"}], "]"}], ",", "0"}], "}"}], ",", 
                 RowBox[{"{", 
                  RowBox[{
                   RowBox[{"#", "[", 
                    RowBox[{"[", "2", "]"}], "]"}], ",", 
                   RowBox[{"f", "[", 
                    RowBox[{
                    RowBox[{
                    RowBox[{"(", 
                    RowBox[{"1", "-", "tt"}], ")"}], 
                    RowBox[{"#", "[", 
                    RowBox[{"[", "1", "]"}], "]"}]}], "+", 
                    RowBox[{"tt", 
                    RowBox[{"#", "[", 
                    RowBox[{"[", "2", "]"}], "]"}]}]}], "]"}]}], "}"}]}], 
                "]"}], "&"}], "/@", " ", 
              RowBox[{"Partition", "[", 
               RowBox[{"\[DoubleStruckCapitalP]", ",", "2", ",", "1"}], 
